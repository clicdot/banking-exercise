{
  "name": "banking",
  "version": "0.2.31",
  "description": "",
  "author": "Philip Choi",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "bump": "$(npm bin)/bump --tag --commit && git push && git push --tags",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copyawscerts": "copyfiles docker/aws/* dist/",
    "doc": "npx compodoc -p tsconfig.json -s",
    "docker:start:dev": "docker-compose up -d",
    "docker:start:prod": "docker-compose up -d",
    "docker:end:dev": "docker-compose down",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "logs:api": "docker compose logs -tf api",
    "start": "docker-compose up -d",
    "start:dev": "nest start --watch",
    "start:dev:1": "ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/main.ts",
    "start:dev:2": "nodemon -L -w src --exec 'ts-node-dev' src/main.ts | pino-pretty",
    "start:dev:db:seed": "ts-node -r tsconfig-paths/register scripts/accounts-seed.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "TEST=true jest --runInBand --detectOpenHandles",
    "test:watch": "TEST=true jest --runInBand --detectOpenHandles --watch",
    "test:cov": "TEST=true jest --runInBand --detectOpenHandles --coverage",
    "test:debug": "TEST=true node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TEST=true jest --detectOpenHandles --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.4",
    "@nestjs/typeorm": "^8.0.3",
    "base64-js": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "daemonix": "^3.0.3",
    "fastify-compress": "^4.0.1",
    "fastify-helmet": "^7.0.0",
    "fastify-jwt": "^4.1.3",
    "fastify-plugin": "^3.0.0",
    "fastify-swagger": "^4.13.1",
    "got": "^12.0.1",
    "mongoose": "6.1.7",
    "nest-router": "^1.0.9",
    "nestjs-i18n": "^8.2.2",
    "nestjs-pino": "^2.5.0",
    "node-json-transform": "^1.1.2",
    "pg": "^8.7.1",
    "pino-http": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/got": "^9.6.12",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "mongodb-memory-server": "^8.2.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.0",
    "prettier": "^2.5.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "version-bump-prompt": "^6.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "src/core/",
      "src/main.ts",
      "src/fastify.bootstrap.ts",
      "src/dev.bootstrap.ts",
      "src/daemonix-app.ts",
      "src/api/components/",
      "src/app.module.ts",
      "src/api/v1/v1.module.ts",
      "src/api/v1/v1.Routes.ts",
      "src/api/v1/modules/banking/banking.module.ts"
    ],
    "verbose": true
  }
}
